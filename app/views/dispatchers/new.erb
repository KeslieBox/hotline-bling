<h1>Sign Up</h1>

<%= render partial: '/application/errors', locals: {errors: @dispatcher.errors.full_messages} %>


<%= form_for @dispatcher do |f| %>
    <%=f.label :username %>
    <%=f.text_field :username %>

    <%=f.label :password %>
    <%=f.password_field :password %>

    <%= f.submit "Create Account" %>

<% end %>